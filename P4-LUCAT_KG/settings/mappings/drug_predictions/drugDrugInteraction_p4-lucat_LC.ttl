@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix p4-lucat: <http://p4-lucat.eu/vocab/> .

<DrugDrugPrediction_drugDrugInteraction_p4-lucat_LC>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "./data/data/drug_predictions/wp4_drug_drug_interactions_LC.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://p4-lucat.eu/entity/{Drug1}_interactsWith_{Drug2}";
        rr:class p4-lucat:DrugDrugPrediction
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:interactor2_Drug;
        rr:objectMap [
        rr:template  "http://p4-lucat.eu/entity/{Drug2}"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:confidence;
        rr:objectMap [
        rml:reference "Probability"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:interaction_isRelatedTo_Disease;
        rr:objectMap [
        rr:template  "http://p4-lucat.eu/entity/LungCancer"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:predictionMethod;
        rr:objectMap [
        rr:constant "Literature Predictions"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:provenance;
        rr:objectMap [
        rr:template  "http://p4-lucat.eu/entity/PubMed"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:interactor1_Drug;
        rr:objectMap [
        rr:template  "http://p4-lucat.eu/entity/{Drug1}"
        ]
    ].

