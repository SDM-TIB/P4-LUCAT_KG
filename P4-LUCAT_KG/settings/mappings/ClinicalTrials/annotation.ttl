@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix p4-lucat: <http://p4-lucat.eu/vocab/> .

<Annotation>
	rml:logicalSource [ rml:source "UMLS_combined.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://p4-lucat.eu/entity/{CUI}";
		rr:class p4-lucat:Annotation
	];
	rr:predicateObjectMap [
		rr:predicate p4-lucat:annID;
		rr:objectMap [ 
			rml:reference "CUI" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate p4-lucat:annLabel;
		rr:objectMap [ 
			rml:reference "Label" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate prov:has_provenance;
		rr:objectMap [
			rr:constant "http://p4-lucat.eu/entity/UMLS"
		]
	];
	rr:predicateObjectMap [
		rr:predicate p4-lucat:semanticTypeID;
		rr:objectMap [ 
			rml:reference "Semantic_type_ids" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate p4-lucat:semanticTypeLabel;
		rr:objectMap [ 
			rml:reference "Semantic_type_label" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate p4-lucat:semanticTypeGroup;
		rr:objectMap [ 
			rml:reference "Semantic_group" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:objectMap [ 
			rr:parentTriplesMap <Wikidata>
		]
	];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:objectMap [ 
			rr:parentTriplesMap <DBpedia>
		]
	].

<Wikidata>
	rml:logicalSource [ rml:source "UMLS_combined.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "{Wikidata}";
	];
	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [ 
			rml:reference "Wikidata_type" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate prov:has_provenance;
		rr:objectMap [
			rr:constant "http://p4-lucat.eu/entity/Wikidata"
		]
	].

<DBpedia>
	rml:logicalSource [ rml:source "UMLS_combined.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "{DBpedia}";
	];
	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [ 
			rml:reference "DBpedia_type" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate prov:has_provenance;
		rr:objectMap [
			rr:constant "http://p4-lucat.eu/entity/DBpedia"
		]
	].	
