@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix p4-lucat: <http://p4-lucat.eu/vocab/> .

<UMLS>
	rml:logicalSource [ rml:source "/data/drug/CUIs_oneLabel.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://p4-lucat.eu/Annotation/{Id}";
		rr:class p4-lucat:Annotation
	];
	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [ 
			rr:template "http://p4-lucat.eu/SemanticType/{SemanticType}";
		]
	];
	
	rr:predicateObjectMap [
		rr:predicate p4-lucat:annLabel;
		rr:objectMap [ 
			rml:reference "Label"; 
		]
	];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:parentTriplesMap <DBpedia>
	];

	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:parentTriplesMap <Wikidata>
	];

	rr:predicateObjectMap [
		rr:predicate prov:hadPrimarySource;
		rr:objectMap [
			rr:template "http://p4-lucat.eu/SourceName/UMLS";
		]
	].

	<DBpedia>
	rml:logicalSource [ rml:source "/data/drug/CUIs_oneLabel.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "{DBpediaURL}";
		];
	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [ 
			rr:template "{DBpediaClass}";
		]
	];	
	rr:predicateObjectMap [
		rr:predicate prov:hadPrimarySource;
		rr:objectMap [
			rr:template "http://p4-lucat.eu/SourceName/DBpedia";
		]
	].
		
	<Wikidata>
	rml:logicalSource [ rml:source "/data/drug/CUIs_oneLabel.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "{WikidataURL}";
		];
	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [ 
			rr:template "{WikidataClass}";
		]
	];	
	rr:predicateObjectMap [
		rr:predicate prov:hadPrimarySource;
		rr:objectMap [
			rr:template "http://p4-lucat.eu/SourceName/Wikidata";
		]
	].	
