@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix p4-lucat: <http://p4-lucat.eu/vocab/> .

<Annotation_target>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "./data/data/drug/target.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://p4-lucat.eu/entity/{CUI}";
        rr:class p4-lucat:Annotation
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:annID;
        rr:objectMap [
        rml:reference "CUI"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate prov:hasGeneratedBy;
        rr:objectMap [
        rr:constant "http://p4-lucat.eu/entity/UMLS"
        ]
    ].

<Target_target>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "./data/data/drug/target.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://p4-lucat.eu/entity/{Target_name}";
        rr:class p4-lucat:Target
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:uniprotID;
        rr:objectMap [
        rml:reference "Uni_ID"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:uniprotID;
        rr:objectMap [
        rr:template  "http://p4-lucat.eu/entity/{Uni_ID}"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:target_isRelatedTo_gene;
        rr:objectMap [
        rr:template  "http://p4-lucat.eu/entity/{Gene_name}"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:targetID;
        rr:objectMap [
        rml:reference "Target_ID"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:targetLabel;
        rr:objectMap [
        rml:reference "Target_name"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:externalLink;
        rr:objectMap [
        rr:template  "https://www.uniprot.org/entity/{Uni_ID}"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:hasCUIAnnotation;
        rr:objectMap [
        rr:parentTriplesMap <Annotation_target>;
        ]
    ].

