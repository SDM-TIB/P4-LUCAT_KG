@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix p4-lucat: <http://p4-lucat.eu/vocab/> .

<OncologicalDrug>
	rml:logicalSource [ rml:source "./data/data/drug/oncologicalDrugs.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://p4-lucat.eu/entity/{Name}";
	];
	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [ 
			rr:template "http://p4-lucat.eu/vocab/OncologicalDrug"
		]
	].

<Drug>
	rml:logicalSource [ rml:source "./data/data/drug/oncologicalDrugs.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://p4-lucat.eu/entity/{Name}";
	];
	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [ 
			rr:template "http://p4-lucat.eu/vocab/{DrugType}Drug"
		]
	];
	rr:predicateObjectMap [
		rr:predicate rdfs:subClassOf;
		rr:objectMap [
			rr:parentTriplesMap <OncologicalDrug>
		];
	];
	rr:predicateObjectMap [
		rr:predicate p4-lucat:drugLabel;
		rr:objectMap [
			rml:reference "Name";
		]
	];
	rr:predicateObjectMap [
		rr:predicate p4-lucat:hasCUIAnnotation;
		rr:objectMap [ 
			rr:parentTriplesMap <Annotation>
		]
	].

<Annotation>
	rml:logicalSource [ rml:source "./data/data/drug/oncologicalDrugs.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://p4-lucat.eu/entity/{CUI}";
		rr:class p4-lucat:Annotation
	];
	rr:predicateObjectMap [
		rr:predicate p4-lucat:annID;
		rr:objectMap [ 
			rml:reference "CUI" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate p4-lucat:annLabel;
		rr:objectMap [ 
			rml:reference "Name" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate prov:has_provenance;
		rr:objectMap [
			rr:constant "http://p4-lucat.eu/entity/UMLS"
		]
	].