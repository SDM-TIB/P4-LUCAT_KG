@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix p4-lucat: <http://p4-lucat.eu/vocab/> .

<DrugDrugInteraction_drugDrugInteraction_non-symmetric>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "./data/data/drug/Unsymmetric_DDI_corpus.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://p4-lucat.eu/entity/{PrecipitantDrugID}_interactsWith_{ObjectDrugID}";
        rr:class p4-lucat:DrugDrugInteraction
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:object_drug;
        rr:objectMap [
        rr:template  "http://p4-lucat.eu/entity/{ObjectDrugID}"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:effect;
        rr:objectMap [
        rr:template  "http://p4-lucat.eu/entity/{Adverse_Event}_{Impact}"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate prov:hasGeneratedBy;
        rr:objectMap [
        rr:constant  "http://p4-lucat.eu/entity/DrugBank"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:precipitant_drug;
        rr:objectMap [
        rr:template  "http://p4-lucat.eu/entity/{PrecipitantDrugID}"
        ]
    ].

