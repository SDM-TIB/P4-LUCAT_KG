@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix p4-lucat: <http://p4-lucat.eu/vocab/> .

<DrugTargetInteraction_drugTargetInteraction>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "./data/data/drug/drugTargetInteraction.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://p4-lucat.eu/entity/{Drug}_interactsWith_{Protein ID}";
        rr:class p4-lucat:DrugTargetInteraction
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:interactor2;
        rr:objectMap [
        rr:parentTriplesMap <Target_drugTargetInteraction>;
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:provenance;
        rr:objectMap [
        rr:template  "http://p4-lucat.eu/SourceName/DrugBank"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:interactor1;
        rr:objectMap [
        rr:template  "http://p4-lucat.eu/entity/{Drug}"
        ]
    ].

<Target_drugTargetInteraction>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "./data/data/drug/drugTargetInteraction.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://p4-lucat.eu/entity/{Protein ID}";
        rr:class p4-lucat:Target
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:targetLabel;
        rr:objectMap [
        rml:reference "Protein Name"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:targetID;
        rr:objectMap [
        rml:reference "Protein ID"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate owl:sameAs;
        rr:objectMap [
        rr:parentTriplesMap <Protein_drugTargetInteraction>;
        ]
    ].

<Protein_drugTargetInteraction>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "./data/data/drug/drugTargetInteraction.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://p4-lucat.eu/entity/{Protein ID}";
        rr:class p4-lucat:Protein
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:proteinID;
        rr:objectMap [
        rml:reference "Protein ID"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate owl:sameAs;
        rr:objectMap [
        rr:template  "http://p4-lucat.eu/entity/{Protein ID}"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:proteinLabel;
        rr:objectMap [
        rml:reference "Protein Name"
        ]
    ].

