@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix p4-lucat: <http://p4-lucat.eu/vocab/> .

<#HasTopic>
	rml:logicalSource [ rml:source "/data/data/pub/has_topic_LC.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://p4-lucat.eu/HAS_TOPIC/{subject}_HAS_TOPIC_{object}";
		rr:class p4-lucat:HAS_TOPIC
	];
	rr:predicateObjectMap [
		rr:predicate p4-lucat:confidence;
		rr:objectMap [ 
			rml:reference "confidence" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate p4-lucat:hasAnnotation;
		rr:objectMap [
			rr:template "http://p4-lucat.eu/Annotation/{object}"
		]
	];
	rr:predicateObjectMap [
		rr:predicate p4-lucat:annotates;
		rr:objectMap [
			rr:template "http://p4-lucat.eu/Publication/{subject}"
		]
	];
	rr:predicateObjectMap [
		rr:predicate p4-lucat:hasTopicRelation_isRelatedTo_Disease;
		rr:objectMap [ 
			rr:template "http://p4-lucat.eu/Disorder/LungCancer"; 
		]
	].	
