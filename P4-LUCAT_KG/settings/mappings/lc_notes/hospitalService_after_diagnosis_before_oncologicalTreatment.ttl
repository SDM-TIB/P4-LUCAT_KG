@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix p4-lucat: <http://p4-lucat.eu/vocab/> .

<LCPatient>
	rml:logicalSource [ rml:source "/data/data/lc_notes/after_diagnosis_before_line1.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://p4-lucat.eu/LCPatient/{MEDICALRECORD}";
		rr:class p4-lucat:LCPatient
	];
	rr:predicateObjectMap [
		rr:predicate p4-lucat:hasHospitalService;
		rr:objectMap [ 
			rr:parentTriplesMap <ServiceAfterDiagnosisBeforeOncologicalTreatment>
		]
	].

<ServiceAfterDiagnosisBeforeOncologicalTreatment>
	rml:logicalSource [ rml:source "/data/data/lc_notes/after_diagnosis_before_line1.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://p4-lucat.eu/ServiceAfterDiagnosisBeforeOncologicalTreatment/{MEDICALRECORD}_{Id}";
		rr:class p4-lucat:ServiceAfterDiagnosisBeforeOncologicalTreatment;
	];
	rr:predicateObjectMap [
		rr:predicate p4-lucat:serviceID;
		rr:objectMap [ 
			rr:template "http://p4-lucat.eu/HospitalService/{Id}";
		]
	];		
	rr:predicateObjectMap [
		rr:predicate p4-lucat:initDate;
		rr:objectMap [ 
			rml:reference "initDate";
			rr:datatype xsd:date;
		]
	];		
	rr:predicateObjectMap [
		rr:predicate p4-lucat:diagnosisDate;
		rr:objectMap [ 
			rml:reference "diagnosisDate";
			rr:datatype xsd:date;
		]
	];	
	rr:predicateObjectMap [
		rr:predicate p4-lucat:hospitalized_days;
		rr:objectMap [ 
			rml:reference "NumberDatesinService";
		]
	];
	rr:predicateObjectMap [
		rr:predicate p4-lucat:days_between_firstConsultation_and_diagnosis;
		rr:objectMap [ 
			rml:reference "NumberDatesPeriod";
		]
	];			
	rr:predicateObjectMap [
		rr:predicate p4-lucat:service_has_scope;
		rr:objectMap [ 
			rr:template "http://p4-lucat.eu/ScopeOfService/{SCOPE}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate p4-lucat:service_has_type;
		rr:objectMap [ 
			rr:template "http://p4-lucat.eu/TypeOfService/{TYPE}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate p4-lucat:service_isRelatedTo_department;
		rr:objectMap [ 
			rr:template "http://p4-lucat.eu/ServiceDepartment/{SERVICE}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate p4-lucat:is_associated_with;
		rr:objectMap [ 
			rr:template "http://p4-lucat.eu/ProcessComment/{PROCESS_COMMENT}";
		]
	];	
	rr:predicateObjectMap [
		rr:predicate p4-lucat:is_associated_with;
		rr:objectMap [ 
			rr:template "http://p4-lucat.eu/ProcessStatus/{PROCESS_STATUS}";
		]
	];		
	rr:predicateObjectMap [
		rr:predicate p4-lucat:is_associated_with;
		rr:objectMap [ 
			rr:template "http://p4-lucat.eu/ClinicalStatus/{STATUS_CLINIC_EVENT}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate p4-lucat:is_associated_with;
		rr:objectMap [ 
			rr:template "http://p4-lucat.eu/DiagnosisDescription/{Desc_diagnost}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate p4-lucat:processStartDate;
		rr:objectMap [ 
			rml:reference "PROCESS_START_DATE";
			rr:datatype xsd:date;
		]
	];
	rr:predicateObjectMap [
		rr:predicate p4-lucat:startDate;
		rr:objectMap [ 
			rml:reference "START_DATE";
			rr:datatype xsd:date;
		]
	];
	rr:predicateObjectMap [
		rr:predicate p4-lucat:endDate;
		rr:objectMap [ 
			rml:reference "END_DATE";
			rr:datatype xsd:date;
		]
	];	
	rr:predicateObjectMap [
		rr:predicate p4-lucat:processDescription;
		rr:objectMap [ 
			rml:reference "DESCRIPTION_PROCESS";
		]
	].	

