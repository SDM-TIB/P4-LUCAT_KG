@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix p4-lucat: <http://p4-lucat.eu/vocab/> .
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#> .
@base <http://p4-lucat.eu/vocab/> .


<Patient_CT_Toxicity>
    a rr:TriplesMap;
    rml:logicalSource [ 
      rml:source <RDB_source>;
      rr:sqlVersion rr:SQL2008;
      rml:query """SELECT distinct REPLACE(medicalrecord_CTtox, ' ', '') as medicalrecord, ct_position, ID_toxicity_CT, other_toxicity FROM Patient_CT_toxicity""";
    ];
    rr:subjectMap [
        rr:template "http://p4-lucat.eu/entity/{medicalrecord}_{ct_position}_{ID_toxicity_CT}";
        rr:class p4-lucat:Patient_CT_Toxicity,
                    p4-lucat:Treatment_Toxicity;
    ];
    rr:predicateObjectMap [
    rr:predicate p4-lucat:hasID_LCPatient;
    rr:objectMap [
      rr:template "http://p4-lucat.eu/entity/{medicalrecord}_LCPatient"
    ]
  ]; 
    rr:predicateObjectMap [
    rr:predicate p4-lucat:hasCTPosition;
    rr:objectMap [
      rr:template "http://p4-lucat.eu/entity/{ct_position}"
    ]
  ]; 
    rr:predicateObjectMap [
    rr:predicate p4-lucat:hasID_Toxicity_CT;
    rr:objectMap [
      rr:template "http://p4-lucat.eu/entity/{ID_toxicity_CT}"
    ]
  ]; 
    rr:predicateObjectMap [
    rr:predicate p4-lucat:hasOtherToxicity;
    rr:objectMap [
      rml:reference "other_toxicity"
    ]
  ].


<RDB_source> a d2rq:Database;
  d2rq:jdbcDSN "UMLS_nov21";
  d2rq:jdbcDriver "com.mysql.cj.jdbc.Driver"; 
  d2rq:username "";
  d2rq:password "" .
