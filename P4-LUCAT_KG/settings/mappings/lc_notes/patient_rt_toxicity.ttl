@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix p4-lucat: <http://p4-lucat.eu/vocab/> .
@base <http://p4-lucat.eu/vocab/> .


<Patient_RT_Toxicity>
    rml:logicalSource [ 
      rml:source <RDB_source>;
      rr:sqlVersion rr:SQL2008;
      rml:query """SELECT distinct REPLACE(medicalrecord_RTtox, ' ', '') as medicalrecord, rt_position, ID_toxicity_RT, other_toxicity FROM Patient_RT_toxicity""";
    ];
    rr:subjectMap [
        rr:template "http://p4-lucat.eu/entity/{medicalrecord}_{rt_position}_{ID_toxicity_RT}";
        rr:class p4-lucat:Patient_RT_Toxicity
    ];
    rr:predicateObjectMap [
    rr:predicate p4-lucat:hasID_LCPatient;
    rr:objectMap [
      rr:template "http://p4-lucat.eu/entity/{medicalrecord}_LCPatient"
    ]
  ]; 
    rr:predicateObjectMap [
    rr:predicate p4-lucat:hasRTPosition;
    rr:objectMap [
      rr:template "http://p4-lucat.eu/entity/{rt_position}"
    ]
  ]; 
    rr:predicateObjectMap [
    rr:predicate p4-lucat:hasID_Toxicity_RT;
    rr:objectMap [
      rr:template "http://p4-lucat.eu/entity/{ID_toxicity_RT}"
    ]
  ]; 
    rr:predicateObjectMap [
    rr:predicate p4-lucat:hasOtherToxicity;
    rr:objectMap [
      rr:template "http://p4-lucat.eu/entity/{other_toxicity}"
    ]
  ].


<RDB_source> a d2rq:Database;
  d2rq:jdbcDSN "UMLS_nov21";
  d2rq:jdbcDriver "com.mysql.cj.jdbc.Driver"; 
  d2rq:username "";
  d2rq:password "" .