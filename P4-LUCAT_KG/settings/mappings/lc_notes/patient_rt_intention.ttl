@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix p4-lucat: <http://p4-lucat.eu/vocab/> .
@base <http://p4-lucat.eu/vocab/> .


<Patient_RT_Intention>
    rml:logicalSource [ 
      rml:source <RDB_source>;
      rr:sqlVersion rr:SQL2008;
      rml:query """SELECT DISTINCT REPLACE(t1.medicalrecord_RT, ' ', '') as medicalrecord, t1.rt_position, t1.fecinirt, t1.fecfinrt, t1.intencion as intencionLabelSP, t1.intencion_CUI, t2.Label as  intencionLabelEN,  t1.tiport, t1.otrort, t1.respuestart, t1.indtoxrt, t1.dosis, t1.fraccion FROM Patient_RT as t1, UMLS_nov21.oneLabel as t2 where t1.intencion_CUI=t2.CUI""";
    ];
    rr:subjectMap [
        rr:template "http://p4-lucat.eu/entity/{medicalrecord}_{rt_position}";
        rr:class p4-lucat:Patient_RT_Intention
    ];
    rr:predicateObjectMap [
    rr:predicate p4-lucat:hasID_LCPatient;
    rr:objectMap [
      rr:template "http://p4-lucat.eu/entity/{medicalrecord}_LCPatient"
    ]
  ]; 
    rr:predicateObjectMap [
    rr:predicate p4-lucat:hasRTPosition;
    rr:objectMap [
      rr:template "http://p4-lucat.eu/entity/{rt_position}"
    ]
  ]; 
    rr:predicateObjectMap [
        rr:predicate p4-lucat:hasDateStart;
        rr:objectMap [
        rml:reference "fecinirt";
        rr:datatype xsd:date
        ]
    ];
     rr:predicateObjectMap [
        rr:predicate p4-lucat:hasDateEnd;
        rr:objectMap [
        rml:reference "fecfinrt";
        rr:datatype xsd:date
        ]
    ];
    rr:predicateObjectMap [
    rr:predicate p4-lucat:intentionLabel_SPA;
    rr:objectMap [
      rr:template "http://p4-lucat.eu/entity/{intencionLabelSP}"
    ]
  ];   
    rr:predicateObjectMap [
        rr:predicate p4-lucat:hasCUI_Intention;
        rr:objectMap [
        rr:template "http://p4-lucat.eu/entity/{intencion_CUI}
        ]
    ];  
    rr:predicateObjectMap [
        rr:predicate p4-lucat:intentionLabel_ENG;
        rr:objectMap [
        rr:template "http://p4-lucat.eu/entity/{intencionLabelEN}
        ]
    ];  
    rr:predicateObjectMap [
        rr:predicate p4-lucat:hasTreatmentArea;
        rr:objectMap [
        rr:template "http://p4-lucat.eu/entity/{tiport}
        ]
    ];  
    rr:predicateObjectMap [
        rr:predicate p4-lucat:hasOtherArea;
        rr:objectMap [
        rr:template "http://p4-lucat.eu/entity/{otrort}
        ]
    ];  
    rr:predicateObjectMap [
        rr:predicate p4-lucat:hasRespueStart;
        rr:objectMap [
        rr:template "http://p4-lucat.eu/entity/{respuestart}
        ]
    ];  
    rr:predicateObjectMap [
        rr:predicate p4-lucat:hasIndtoxrt;
        rr:objectMap [
        rr:template "http://p4-lucat.eu/entity/{indtoxrt}
        ]
    ];  
    rr:predicateObjectMap [
        rr:predicate p4-lucat:hasDosage;
        rr:objectMap [
        rr:template "http://p4-lucat.eu/entity/{dosis}
        ]
    ];  
    rr:predicateObjectMap [
        rr:predicate p4-lucat:hasFraction;
        rr:objectMap [
        rr:template "http://p4-lucat.eu/entity/{fraccion}
        ]
    ].


<RDB_source> a d2rq:Database;
  d2rq:jdbcDSN "UMLS_nov21";
  d2rq:jdbcDriver "com.mysql.cj.jdbc.Driver"; 
  d2rq:username "";
  d2rq:password "" .