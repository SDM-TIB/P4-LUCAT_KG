@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix p4-lucat: <http://p4-lucat.eu/vocab/> .

<DiagnosisDescription>
	rml:logicalSource [ rml:source "./data/data/lc_notes/description_analysis.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://p4-lucat.eu/DiagnosisDescription/{Id}";
		rr:class p4-lucat:DiagnosisDescription;
	];
	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [ 
			rml:reference "DESC_DIAGNOST_English" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate p4-lucat:label_SPA;
		rr:objectMap [ 
			rml:reference "DESC_DIAGNOST_Spanish" 
		]
	].
