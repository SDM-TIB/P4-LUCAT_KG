@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix p4-lucat: <http://p4-lucat.eu/vocab/> .
@base <http://p4-lucat.eu/vocab/> .


<Surgery_Information>
    rml:logicalSource [ 
      rml:source <RDB_source>;
      rr:sqlVersion rr:SQL2008;
      rml:query """SELECT distinct REPLACE(medicalrecord_surgeryinfo, ' ', '') as medicalrecord, cirtor, num_surgeries  FROM Surgery_information""";
    ];
    rr:subjectMap [
        rr:template "http://p4-lucat.eu/entity/{medicalrecord}_LCPatient";
        rr:class p4-lucat:Surgery_Information
    ];
    rr:predicateObjectMap [
    rr:predicate p4-lucat:hasCirtor;
    rr:objectMap [
      rr:template "http://p4-lucat.eu/entity/{cirtor}"
    ]
  ];
  rr:predicateObjectMap [
    rr:predicate p4-lucat:hasNumberSurgery;
    rr:objectMap [
      rml:reference "num_surgeries"
    ]
  ].


<RDB_source> a d2rq:Database;
  d2rq:jdbcDSN "UMLS_nov21";
  d2rq:jdbcDriver "com.mysql.cj.jdbc.Driver"; 
  d2rq:username "";
  d2rq:password "" .
