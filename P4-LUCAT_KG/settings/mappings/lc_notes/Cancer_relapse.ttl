@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix p4-lucat: <http://p4-lucat.eu/vocab/> .
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#> .
@base <http://p4-lucat.eu/vocab/> .

<Patient_Relapse>
   a rr:TriplesMap;
   rml:logicalSource [ 
      rml:source <RDB_source>;
      rr:sqlVersion rr:SQL2008;
      rml:query """SELECT distinct REPLACE(t1.medicalrecord_relapse, ' ', '') as medicalrecord, t1.relapse_position,t2.recaida_translated, t2.recaida_CUI, t1.fecrec, t1.tipoprog, t1.terapiar FROM Cancer_relapse as t1, Relapse as t2 WHERE t2.recaida=t1.recaida""";
    ];
    rr:subjectMap [
        rr:template "http://p4-lucat.eu/entity/{medicalrecord}_{relapse_position}";
        rr:class p4-lucat:Patient_Relapse
    ];
     rr:predicateObjectMap [
        rr:predicate p4-lucat:hasID_LCPatient;
        rr:objectMap [
        rr:template "http://p4-lucat.eu/entity/{medicalrecord}_LCPatient"
        ]
    ];
    rr:predicateObjectMap [
    rr:predicate p4-lucat:hasRelapsePosition;
    rr:objectMap [
      rr:template "http://p4-lucat.eu/entity/{relapse_position}"
    ]
  ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:hasRelapseProgression;
        rr:objectMap [
        rr:template "http://p4-lucat.eu/entity/{recaida_translated}"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:hasCUIAnnotation;
        rr:objectMap [
        rr:template "http://p4-lucat.eu/entity/{recaida_CUI}"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:hasRelapseDate;
        rr:objectMap [
        rml:reference "fecrec";
        rr:datatype xsd:date
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:hasTypeRelapseProgression;
        rr:objectMap [
        rml:reference "tipoprog"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate p4-lucat:hasTypeTherapyRelapseProgression;
        rr:objectMap [
        rr:template "http://p4-lucat.eu/entity/{terapiar}"
        ]
    ].

<RDB_source> a d2rq:Database;
  d2rq:jdbcDSN "UMLS_nov21";
  d2rq:jdbcDriver "com.mysql.cj.jdbc.Driver"; 
  d2rq:username "";
  d2rq:password "" .
